{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": "please-use-yarn-instead",
    "yarn": ">= 1.2.0",
    "node": ">=10.0"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon",
    "test": "echo test",
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "typeorm": "typeorm --config ./src/config/typeorm.config.ts",
    "typeorm:migrate": "npm run typeorm migration:run",
    "typeorm:fixtures": "ts-node ./src/fixtures/load-fixtures.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "devDependencies": {
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/graphql": "^14.5.0",
    "apollo-server-express": "^2.12.0",
    "dataloader": "^2.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "graphql": "14.1.1",
    "graphql-voyager": "^1.0.0-rc.29",
    "mysql": "^2.14.1",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.10",
    "type-graphql": "^0.17.6",
    "typeorm": "0.2.24"
  }
}
